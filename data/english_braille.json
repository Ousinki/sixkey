{
  "meta": {
    "name": "English Grade 1 Braille (EBAE)",
    "standard": "English Braille American Edition (EBAE), Uncontracted",
    "description": "Direct letter-to-braille mapping with no contractions. Each letter, digit, and punctuation mark has a unique braille pattern.",
    "source": "liblouis en-us-g1.ctb"
  },
  "letters": {
    "a": { "dots": [1],          "unicode": "U+2801", "char": "⠁" },
    "b": { "dots": [1, 2],       "unicode": "U+2803", "char": "⠃" },
    "c": { "dots": [1, 4],       "unicode": "U+2809", "char": "⠉" },
    "d": { "dots": [1, 4, 5],    "unicode": "U+2819", "char": "⠙" },
    "e": { "dots": [1, 5],       "unicode": "U+2811", "char": "⠑" },
    "f": { "dots": [1, 2, 4],    "unicode": "U+280B", "char": "⠋" },
    "g": { "dots": [1, 2, 4, 5], "unicode": "U+281B", "char": "⠛" },
    "h": { "dots": [1, 2, 5],    "unicode": "U+2813", "char": "⠓" },
    "i": { "dots": [2, 4],       "unicode": "U+280A", "char": "⠊" },
    "j": { "dots": [2, 4, 5],    "unicode": "U+281A", "char": "⠚" },
    "k": { "dots": [1, 3],       "unicode": "U+2805", "char": "⠅" },
    "l": { "dots": [1, 2, 3],    "unicode": "U+2807", "char": "⠇" },
    "m": { "dots": [1, 3, 4],    "unicode": "U+280D", "char": "⠍" },
    "n": { "dots": [1, 3, 4, 5], "unicode": "U+281D", "char": "⠝" },
    "o": { "dots": [1, 3, 5],    "unicode": "U+2815", "char": "⠕" },
    "p": { "dots": [1, 2, 3, 4], "unicode": "U+280F", "char": "⠏" },
    "q": { "dots": [1, 2, 3, 4, 5], "unicode": "U+281F", "char": "⠟" },
    "r": { "dots": [1, 2, 3, 5], "unicode": "U+2817", "char": "⠗" },
    "s": { "dots": [2, 3, 4],    "unicode": "U+280E", "char": "⠎" },
    "t": { "dots": [2, 3, 4, 5], "unicode": "U+281E", "char": "⠞" },
    "u": { "dots": [1, 3, 6],    "unicode": "U+2825", "char": "⠥" },
    "v": { "dots": [1, 2, 3, 6], "unicode": "U+2827", "char": "⠧" },
    "w": { "dots": [2, 4, 5, 6], "unicode": "U+283A", "char": "⠺" },
    "x": { "dots": [1, 3, 4, 6], "unicode": "U+282D", "char": "⠭" },
    "y": { "dots": [1, 3, 4, 5, 6], "unicode": "U+283D", "char": "⠽" },
    "z": { "dots": [1, 3, 5, 6], "unicode": "U+2835", "char": "⠵" }
  },
  "digits": {
    "_indicator": { "dots": [3, 4, 5, 6], "unicode": "U+283C", "char": "⠼", "name": "Number sign (precedes digits)" },
    "0": { "dots": [2, 4, 5],    "unicode": "U+281A", "char": "⠚", "note": "Same as j" },
    "1": { "dots": [1],          "unicode": "U+2801", "char": "⠁", "note": "Same as a" },
    "2": { "dots": [1, 2],       "unicode": "U+2803", "char": "⠃", "note": "Same as b" },
    "3": { "dots": [1, 4],       "unicode": "U+2809", "char": "⠉", "note": "Same as c" },
    "4": { "dots": [1, 4, 5],    "unicode": "U+2819", "char": "⠙", "note": "Same as d" },
    "5": { "dots": [1, 5],       "unicode": "U+2811", "char": "⠑", "note": "Same as e" },
    "6": { "dots": [1, 2, 4],    "unicode": "U+280B", "char": "⠋", "note": "Same as f" },
    "7": { "dots": [1, 2, 4, 5], "unicode": "U+281B", "char": "⠛", "note": "Same as g" },
    "8": { "dots": [1, 2, 5],    "unicode": "U+2813", "char": "⠓", "note": "Same as h" },
    "9": { "dots": [2, 4],       "unicode": "U+280A", "char": "⠊", "note": "Same as i" }
  },
  "indicators": {
    "capital": { "dots": [6],       "unicode": "U+2820", "char": "⠠", "name": "Capital letter indicator" },
    "capital_word": { "dots": [6, 6], "unicode": "U+2820U+2820", "char": "⠠⠠", "name": "Capital word indicator (double)" },
    "number": { "dots": [3, 4, 5, 6], "unicode": "U+283C", "char": "⠼", "name": "Number indicator" },
    "letter": { "dots": [5, 6],       "unicode": "U+2830", "char": "⠰", "name": "Letter sign (after number)" }
  },
  "punctuation": {
    ".": { "dots": [2, 5, 6],       "unicode": "U+2832", "char": "⠲", "name": "Period" },
    ",": { "dots": [2],             "unicode": "U+2802", "char": "⠂", "name": "Comma" },
    "?": { "dots": [2, 3, 6],       "unicode": "U+2826", "char": "⠦", "name": "Question mark" },
    "!": { "dots": [2, 3, 5],       "unicode": "U+2816", "char": "⠖", "name": "Exclamation mark" },
    ";": { "dots": [2, 3],          "unicode": "U+2806", "char": "⠆", "name": "Semicolon" },
    ":": { "dots": [2, 5],          "unicode": "U+2812", "char": "⠒", "name": "Colon" },
    "'": { "dots": [3],             "unicode": "U+2804", "char": "⠄", "name": "Apostrophe" },
    "-": { "dots": [3, 6],          "unicode": "U+2824", "char": "⠤", "name": "Hyphen" }
  },
  "examples": {
    "hello": {
      "braille": "⠓⠑⠇⠇⠕",
      "breakdown": ["h → ⠓", "e → ⠑", "l → ⠇", "l → ⠇", "o → ⠕"]
    },
    "Hello": {
      "braille": "⠠⠓⠑⠇⠇⠕",
      "breakdown": ["⠠ (capital indicator)", "h → ⠓", "e → ⠑", "l → ⠇", "l → ⠇", "o → ⠕"]
    },
    "42": {
      "braille": "⠼⠙⠃",
      "breakdown": ["⠼ (number indicator)", "4 → ⠙", "2 → ⠃"]
    }
  }
}
